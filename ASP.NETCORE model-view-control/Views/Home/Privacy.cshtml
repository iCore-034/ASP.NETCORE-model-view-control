@using DataNs;
@{
    ViewData["Title"] = "Privacy Policy";
}
<div class="limiter">

	<div class="rofl-chmo">

		<div class="container-login100" style="background-image: url('/Login_v16/images/bg-01.jpg');">
			<div class="wrap-login100 p-t-30 p-b-50">
				<span class="login100-form-title p-b-41">
					<h3 class="login100-form-btn">REGISTRATION & AUTHENTICATION</h3>
				</span>
				<form id='targetDiv' class="login100-form validate-form p-b-33 p-t-5"
				method="get" asp-controller="Home" asp-action="Registration">

					<div class="wrap-input100 validate-input" data-validate="Enter username">
						<input class="input100" type="text" name="login" placeholder="Login">
						<span class="focus-input100"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Enter password">
						<input class="input100" type="password" name="password" placeholder="Password">
						<span class="focus-input100"></span>
					</div>

					<div class="container-login100-form-btn m-t-32">
						<input class="login100-form-btn" type="submit" value="Registration"/>

					</div>
				</form>
				<form method="get" asp-controller="Home" asp-action="Index">

					<div class="container-login100-form-btn m-t-32">
						
						<input class="login100-form-btn" type="submit" value="🠐 Return"/>
					</div>
				</form>
				<div class="container-login100-form-btn m-t-32 input100" id="ErrorLoginOrPass">
					@Data.userLoginPassError
				</div>
			</div>
		</div>
	</div>
</div>
<script src="~/Login_v16/js/script.js"></script>